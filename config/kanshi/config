profile "external" {
  output "eDP-1" disable
  output "HDMI-A-1" enable
}

profile "laptop-only" {
  output "eDP-1" enable
  output "HDMI-A-1" disable
}

output "ASUSTek COMPUTER INC PG27AQDP S9LMRS028756" {
  mode 2560x1440@480
  position 0,0
  scale 1
  adaptive_sync off
  alias $home_1
}

output "AOC 24G4 12VQ1HA024438" {
  mode 1920x1080@180
  position 0,0
  scale 1
  adaptive_sync off
  alias $home_2
}


profile home_1 {
  output $home_1 enable
}

